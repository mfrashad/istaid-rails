<div class="container">
<h3> Pages </h3>
<%= link_to admin_root_path, class:'btn waves-effect waves-light' do %>
  <i class="material-icons left">arrow_back</i>Back
<% end%>
<div class="card-panel">
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @static_pages.each do |static_page| %>
      <tr>
        <td><%= static_page.name %></td>
        <td><%= link_to 'Edit', edit_admin_static_page_path(static_page) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="center">
    <%= will_paginate @static_pages, renderer: @custom_paginate_renderer %>
</div>
<%= link_to 'New static_page', new_admin_static_page_path, class:'btn waves-effect waves-light' %><br>
</div>
</div>
